{
  "name": "vscode-angular-support",
  "displayName": "Angular Support",
  "description": "Go to / peek angular specific definitions. Supports Angular 21.",
  "version": "0.2.0",
  "publisher": "skwonandy",
	"icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/skwonandy/vscode-angular-support"
  },
  "engines": {
    "vscode": "^1.10.0"
  },
  "keywords": [
    "angular",
    "typescript",
    "support",
    "peek",
    "definition"
  ],
  "categories": [
    "Languages",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:html"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "build": "tsc -p ./",
    "test": "npm run build"
  },
  "dependencies": {
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^14.18.63",
    "@types/vscode": "^1.10.0",
    "mocha": "^10.2.0",
    "sharp": "^0.34.5"
  }
}
